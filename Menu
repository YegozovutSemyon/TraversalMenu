--made by Semyon

local Rayfield = loadstring(game:HttpGet('https://sirius.menu/rayfield'))()

local Window = Rayfield:CreateWindow({
   Name = "Traversal HUB",
   Icon = 0, 
   LoadingTitle = "Traversal HUB",
   LoadingSubtitle = "Made by Semyon",
   Theme = "Default", 

   DisableRayfieldPrompts = false,
   DisableBuildWarnings = false, 

   ConfigurationSaving = {
      Enabled = true,
      FolderName = nil, 
      FileName = "Big Hub"
   },

   Discord = {
      Enabled = false, 
      Invite = "noinvitelink", 
      RememberJoins = true 
   },

   KeySystem = false, 
   KeySettings = {
      Title = "Untitled",
      Subtitle = "Key System",
      Note = "No method of obtaining the key is provided", 
      FileName = "Key", 
      SaveKey = true, 
      GrabKeyFromSite = false, 
      Key = {"Hello"} 
   }
})

local PlayerTab = Window:CreateTab("GODMODE", "heart-pulse")
local Label = PlayerTab:CreateLabel("U HAVE TO REJOIN IF ONE OF THEM DIDN'T WORK", "triangle-alert")
local Button = PlayerTab:CreateButton({
   Name = "Trun ON",
   Callback = function()
   while true do task.wait() local args = {
    [1] = {
        [1] = {
            [1] = "\25",
            [2] = game:GetService("Players").LocalPlayer.Character.Humanoid,
            [3] = math.huge
        }
    }
}

game:GetService("ReplicatedStorage"):WaitForChild("RemoteEvent"):FireServer(unpack(args))
end
   end,
})

local Button = PlayerTab:CreateButton({
   Name = "Trun ON ( IF THE FIRST ONE DIDN'T WORK )",
   Callback = function()
   while true do task.wait() local args = {
    [1] = {
        [1] = {
            [1] = "\26",
            [2] = game:GetService("Players").LocalPlayer.Character.Humanoid,
            [3] = math.huge
        }
    }
}

game:GetService("ReplicatedStorage"):WaitForChild("RemoteEvent"):FireServer(unpack(args))
end
   end,
})

local Button = PlayerTab:CreateButton({
   Name = "RE JOIN (TELL ME IF IT'S NOT WORKING )",
   Callback = function()
   local ts = game:GetService("TeleportService")

   local p = game:GetService("Players").LocalPlayer

 

    ts:Teleport(game.PlaceId, p)

   end,
})

local PlayerTab = Window:CreateTab("KILL ALL", "skull")
local Label = PlayerTab:CreateLabel("PRESS THE SECOND ONE IF THE FIRST ON DIDN'T WORK", "triangle-alert")
local Button = PlayerTab:CreateButton({
   Name = "KILL ALL ENEMIES",
   Callback = function()
    local Players = game:GetService("Players")
local ReplicatedStorage = game:GetService("ReplicatedStorage")
local RemoteEvent = ReplicatedStorage:WaitForChild("RemoteEvent")
local enemiesFolder = workspace:WaitForChild("Enemies")
local localPlayer = Players.LocalPlayer

for _, enemy in ipairs(enemiesFolder:GetChildren()) do
    if enemy:IsA("Model") then
        local args = {
            [1] = {
                [1] = {
                    [1] = "\8",
                    [2] = false,
                    [3] = "Left Arm",
                    [4] = enemy,
                    [5] = 200
                }
            }
        }
        RemoteEvent:FireServer(unpack(args))
    end
end


   end,
})

